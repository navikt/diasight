{
    "npmScope": "diasight",
    "affected": {
        "defaultBase": "main"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": [
                    "build",
                    "lint",
                    "test",
                    "e2e"
                ]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "projects": {
        "api": {
            "tags": []
        },
        "api-interfaces": {
            "tags": []
        },
        "frontend": {
            "tags": []
        },
        "frontend-e2e": {
            "tags": [],
            "implicitDependencies": [
                "frontend"
            ]
        }
    }
}
